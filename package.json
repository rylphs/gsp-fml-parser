{
  "name": "formula-parser",
  "version": "1.0.0",
  "description": "Google spreadsheet formula parser for writing custom formulas.",
  "main": "index.js",
  "scripts": {
    "build-grammars": "nearleyc ./formula-grammar.ne --out ./formula-grammar.ts && nearleyc ./argument-grammar.ne --out ./argument-grammar.ts",
    "ts-compile": "tsc --target ES5 formula-parser.ts formula-grammar.ts argument-grammar.ts",
    "build": "npm run build-grammars && npm run ts-compile",
    "test": "npm run build && node test.js",
    "test-grammar": "npm run build && nearley-test ./formula-grammar.js -i $1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rylphs/gsp-fml-parser.git"
  },
  "keywords": [
    "spreadsheet",
    "google",
    "formula"
  ],
  "author": "Raphael Couto Ferreira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rylphs/gsp-fml-parser/issues"
  },
  "homepage": "https://github.com/rylphs/gsp-fml-parser#readme",
  "dependencies": {
    "moo": "^0.4.3",
    "nearley": "^2.11.0"
  },
  "devDependencies": {
    "typescript": "^2.5.3"
  }
}
